public class Main 
{
    public static void main(String[] args) 
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=0,d=0;
        while(a!=0)
        {
            int x=a%10;
            int y=b%10;
            if(a==0||b==0)
                break;
            c=x+y;
            c=c/10;
            if(c==1)
                d++;
            a=a/10;
            b=b/10;
        }
        System.out.println(d);
    }
}
